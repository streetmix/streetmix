{
  "name": "streetmix",
  "version": "1.3.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/streetmix/streetmix.git"
  },
  "license": "BSD-3-Clause",
  "engines": {
    "node": "8.12.0",
    "npm": "6.4.1"
  },
  "scripts": {
    "postinstall": "node app/bundle.js",
    "prestart": "npm run mongo:start",
    "start": "npm run app:start",
    "stop": "npm run mongo:stop",
    "test": "npm run lint && npm run jest:watch",
    "pretest:full": "npm run mongo:start && npm run protractor:update && npm run test:server",
    "test:full": "npm run jest && npm run lint && npm run protractor:local",
    "posttest:full": "npm run test:cleanup",
    "precommit": "lint-staged",
    "mongo:start": "mongod --fork --logpath /dev/null",
    "mongo:stop": "mongo admin --eval 'db.shutdownServer()'",
    "app:start": "supervisor --extensions js --ignore .cache,assets,build,test index.js",
    "test:cleanup": "npm run test:server:close && npm run mongo:stop",
    "test:server": "NODE_ENV=test PORT=3000 node index &",
    "test:server:close": "pkill -SIGINT streetmix",
    "lint": "npm run lint:css && npm run lint:js && npm run lint:json",
    "lint:css": "stylelint ./assets/css/**/*.scss",
    "lint:js": "eslint \"**/*.js\" \"**/*.jsx\"",
    "lint:json": "find assets -name '*.json' -exec jsonlint {} --quiet \\;",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "protractor:update": "./node_modules/protractor/bin/webdriver-manager update",
    "protractor:local": "protractor ./test/integration/local.conf.js",
    "translations:download": "node bin/download_translations.js"
  },
  "cacheDirectories": [
    ".cache"
  ],
  "jest": {
    "roots": [
      "app",
      "assets/scripts"
    ],
    "setupFiles": [
      "./test/setup-jest.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|svg)$": "<rootDir>/test/__mocks__/fileMock.js"
    }
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "not IE <= 10"
  ],
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {}
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.jsx": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "jsonlint --in-place",
      "git add"
    ],
    "*.scss": [
      "stylelint --syntax=scss"
    ]
  },
  "dependencies": {
    "@babel/core": "7.1.0",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "@fortawesome/fontawesome-svg-core": "1.2.4",
    "@fortawesome/free-brands-svg-icons": "5.3.1",
    "@fortawesome/free-regular-svg-icons": "5.3.1",
    "@fortawesome/free-solid-svg-icons": "5.3.1",
    "@fortawesome/react-fontawesome": "0.1.3",
    "@streetmix/icons": "0.5.0",
    "@streetmix/illustrations": "0.4.1",
    "async": "2.6.1",
    "auth0": "2.12.0",
    "auth0-js": "9.7.3",
    "autoprefixer": "9.1.5",
    "axios": "0.18.0",
    "btoa": "1.2.1",
    "chalk": "2.4.1",
    "compression": "1.7.3",
    "config": "2.0.1",
    "cookie-parser": "1.4.3",
    "cookie-session": "2.0.0-beta.3",
    "cors": "2.8.4",
    "dateformat": "3.0.3",
    "express": "4.16.3",
    "file-saver": "1.3.8",
    "handjs": "1.3.11",
    "hbs": "4.0.1",
    "helmet": "3.13.0",
    "js-cookie": "2.2.0",
    "leaflet": "1.3.4",
    "lodash": "4.17.11",
    "mongoose": "5.2.17",
    "newrelic": "4.8.1",
    "node-env-file": "0.1.8",
    "node-sass": "4.9.3",
    "oauth": "0.9.15",
    "parcel-bundler": "1.10.0",
    "postcss": "7.0.3",
    "prop-types": "15.6.2",
    "raven-js": "3.27.0",
    "react": "16.5.2",
    "react-autosuggest": "9.4.2",
    "react-dnd": "5.0.0",
    "react-dnd-html5-backend": "5.0.1",
    "react-dnd-multi-backend": "3.1.3",
    "react-dom": "16.5.2",
    "react-intl": "2.6.0",
    "react-leaflet": "2.0.1",
    "react-redux": "5.0.7",
    "react-transition-group": "2.3.1",
    "redis": "2.8.0",
    "redux": "4.0.0",
    "redux-thunk": "2.3.0",
    "request": "2.88.0",
    "requireindex": "1.2.0",
    "sharedstreets": "0.9.3",
    "shifty": "2.6.0",
    "sprintf": "0.1.5",
    "supervisor": "0.12.0",
    "twitter": "1.7.1",
    "uuid": "3.3.2",
    "validator": "10.8.0",
    "whatwg-fetch": "3.0.0",
    "winston": "3.1.0"
  },
  "devDependencies": {
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-plugin-rewire": "1.2.0",
    "enzyme": "3.6.0",
    "enzyme-adapter-react-16": "1.5.0",
    "eslint": "5.6.0",
    "eslint-config-standard": "12.0.0",
    "eslint-config-standard-react": "7.0.2",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-standard": "4.0.0",
    "husky": "0.14.3",
    "jest": "23.6.0",
    "jest-canvas-mock": "1.1.0",
    "jest-date-mock": "1.0.5",
    "jest-fetch-mock": "1.6.6",
    "jsonlint": "1.6.3",
    "lint-staged": "7.3.0",
    "protractor": "5.4.1",
    "react-test-renderer": "16.5.2",
    "redux-mock-store": "1.5.3",
    "sass": "1.14.3",
    "stylelint": "9.5.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-scss": "3.3.1",
    "supertest": "3.3.0"
  }
}
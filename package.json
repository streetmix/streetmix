{
  "name": "streetmix",
  "version": "1.3.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/streetmix/streetmix.git"
  },
  "license": "BSD-3-Clause",
  "engines": {
    "node": ">=14.x",
    "npm": ">=6.x"
  },
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "postinstall": "node app/bundle.js",
    "start": "supervisor --extensions js --ignore .cache,assets,build,test index.js",
    "test": "npm run lint && npm run jest:watch",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "stylelint \"./assets/**/*.scss\"",
    "lint:js": "eslint --ext .jsx,.js .",
    "jest": "jest --collectCoverage",
    "jest:watch": "jest --watch",
    "translations:download": "node bin/download_translations.js",
    "docs": "npm run docs:build && npm run docs:serve",
    "docs:build": "cd docs && make dirhtml",
    "docs:serve": "npx http-server docs/_build/dirhtml",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:create": "npx sequelize-cli db:create",
    "pretest:models": "NODE_ENV=test npm run db:migrate",
    "test:models": "NODE_ENV=test jest --watchAll --testPathPattern=app/db/models/_tests_",
    "posttest:models": "NODE_ENV=test npm run db:reset"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "jest": {
    "roots": [
      "app",
      "assets/scripts",
      "test"
    ],
    "setupFilesAfterEnv": [
      "./test/setup-jest.js"
    ],
    "testPathIgnorePatterns": [
      "./test/integration"
    ],
    "collectCoverageFrom": [
      "app.js",
      "app/**/*.{js,jsx}",
      "assets/scripts/**/*.{js,jsx}",
      "!assets/scripts/vendor/**/*.{js,jsx}"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|svg)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/test/__mocks__/styleMock.js"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx",
      "node",
      "mjs"
    ]
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "not IE <= 10"
  ],
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {},
      "postcss-round-subpixels": {}
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,scss,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@formatjs/intl-relativetimeformat": "4.5.11",
    "@fortawesome/fontawesome-svg-core": "1.2.25",
    "@fortawesome/free-brands-svg-icons": "5.15.2",
    "@fortawesome/free-regular-svg-icons": "5.15.1",
    "@fortawesome/free-solid-svg-icons": "5.14.0",
    "@fortawesome/react-fontawesome": "0.1.13",
    "@primer/octicons-react": "11.2.0",
    "@reduxjs/toolkit": "1.5.0",
    "@sentry/browser": "5.15.5",
    "@streetmix/illustrations": "1.1.0",
    "@tippyjs/react": "4.2.0",
    "auth0": "2.33.0",
    "auth0-js": "9.13.2",
    "autoprefixer": "9.8.6",
    "axios": "0.21.1",
    "btoa": "1.2.1",
    "chalk": "4.1.0",

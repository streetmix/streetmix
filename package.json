{
  "name": "streetmix",
  "version": "1.3.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/streetmix/streetmix.git"
  },
  "license": "BSD-3-Clause",
  "engines": {
    "node": "10.11.0",
    "npm": "6.4.1"
  },
  "scripts": {
    "postinstall": "node app/bundle.js",
    "prestart": "npm run mongo:start",
    "start": "npm run app:start",
    "stop": "npm run mongo:stop",
    "test": "npm run lint && npm run jest:watch",
    "pretest:full": "npm run mongo:start && npm run protractor:update && npm run test:server",
    "test:full": "npm run jest && npm run lint && npm run protractor:local",
    "posttest:full": "npm run test:cleanup",
    "mongo:start": "mongod --fork --logpath /dev/null",
    "mongo:stop": "mongo admin --eval 'db.shutdownServer()'",
    "app:start": "supervisor --extensions js --ignore .cache,assets,build,test index.js",
    "test:cleanup": "npm run test:server:close && npm run mongo:stop",
    "test:server": "NODE_ENV=test PORT=3000 node index &",
    "test:server:close": "pkill -SIGINT streetmix",
    "lint": "npm run lint:css && npm run lint:js && npm run lint:json",
    "lint:css": "stylelint \"./assets/**/*.scss\"",
    "lint:js": "eslint \"**/*.js\" \"**/*.jsx\"",
    "lint:json": "find assets -name '*.json' -exec jsonlint {} --quiet \\;",
    "jest": "jest --collectCoverage",
    "jest:watch": "jest --watch",
    "protractor:update": "./node_modules/protractor/bin/webdriver-manager update",
    "protractor:local": "protractor ./test/integration/local.conf.js",
    "translations:download": "node bin/download_translations.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "roots": [
      "app",
      "assets/scripts"
    ],
    "setupFiles": [
      "./test/setup-jest.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "app.js",
      "app/**/*.{js,jsx}",
      "assets/scripts/**/*.{js,jsx}",
      "!assets/scripts/vendor/**/*.{js,jsx}"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|svg)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/test/__mocks__/styleMock.js"
    }
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "not IE <= 10"
  ],
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {}
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.jsx": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "jsonlint --in-place",
      "git add"
    ],
    "*.scss": [
      "stylelint --syntax=scss"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.18",
    "@fortawesome/free-brands-svg-icons": "5.8.2",
    "@fortawesome/free-regular-svg-icons": "5.8.2",
    "@fortawesome/free-solid-svg-icons": "5.8.2",
    "@fortawesome/react-fontawesome": "0.1.4",
    "@streetmix/icons": "0.5.0",
    "@streetmix/illustrations": "0.6.0",
    "async": "2.6.2",
    "auth0": "2.17.0",
    "auth0-js": "9.10.2",
    "autoprefixer": "9.5.1",
    "axios": "0.18.0",
    "btoa": "1.2.1",
    "chalk": "2.4.2",
    "cloudinary": "1.14.0",
    "compression": "1.7.4",
    "config": "3.1.0",
    "cookie-parser": "1.4.4",
    "cookie-session": "2.0.0-beta.3",
    "core-js": "3.0.1",
    "cors": "2.8.5",
    "dateformat": "3.0.3",
    "express": "4.16.4",
    "file-saver": "2.0.1",
    "handjs": "1.3.11",
    "hbs": "4.0.4",
    "helmet": "3.18.0",
    "js-cookie": "2.2.0",
    "leaflet": "1.5.1",
    "lodash": "4.17.11",
    "mongoose": "5.5.7",
    "newrelic": "5.8.0",
    "node-env-file": "0.1.8",
    "oauth": "0.9.15",
    "parcel-bundler": "1.12.3",
    "postcss": "7.0.16",
    "prop-types": "15.7.2",
    "raven-js": "3.27.0",
    "react": "16.8.6",
    "react-autosuggest": "9.4.3",
    "react-dnd": "7.4.5",
    "react-dnd-html5-backend": "7.4.4",
    "react-dnd-multi-backend": "3.2.1",
    "react-dnd-touch-backend": "0.5.1",
    "react-dom": "16.8.6",
    "react-intl": "2.8.0",
    "react-leaflet": "2.2.1",
    "react-redux": "7.0.3",
    "react-transition-group": "4.0.0",
    "redis": "2.8.0",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0",
    "regenerator-runtime": "0.13.2",
    "request": "2.88.0",
    "requireindex": "1.2.0",
    "sass": "1.20.1",
    "shifty": "2.8.0",
    "slugify": "1.3.4",
    "sprintf": "0.1.5",
    "supervisor": "0.12.0",
    "twitter": "1.7.1",
    "uuid": "3.3.2",
    "validator": "10.11.0",
    "whatwg-fetch": "3.0.0",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "@babel/preset-react": "7.0.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.8.0",
    "babel-plugin-rewire": "1.2.0",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.12.1",
    "enzyme-to-json": "3.3.5",
    "eslint": "5.16.0",
    "eslint-config-standard": "12.0.0",
    "eslint-config-standard-react": "7.0.2",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-node": "9.0.1",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-react": "7.13.0",
    "eslint-plugin-standard": "4.0.0",
    "husky": "2.2.0",
    "jest": "24.8.0",
    "jest-canvas-mock": "2.0.0",
    "jest-date-mock": "1.0.7",
    "jest-fetch-mock": "2.1.2",
    "jsonlint": "1.6.3",
    "lint-staged": "8.1.6",
    "protractor": "5.4.2",
    "react-test-renderer": "16.8.6",
    "react-testing-library": "7.x",
    "recompose": "0.30.0",
    "redux-mock-store": "1.5.3",
    "stylelint": "10.0.1",
    "stylelint-config-standard": "18.3.0",
    "stylelint-scss": "3.6.1",
    "supertest": "4.0.2"
  },
  "greenkeeper": {
    "ignore": [
      "react-dnd-touch-backend"
    ]
  }
}
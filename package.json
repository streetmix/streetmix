{
  "name": "streetmix",
  "version": "1.3.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/streetmix/streetmix.git"
  },
  "license": "AGPL-3.0-or-later",
  "engines": {
    "node": "^18.x || ^20.x",
    "npm": ">=9.x"
  },
  "workspaces": [
    "docs",
    "packages/*"
  ],
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "start": "concurrently --names \"SERVER,CLIENT\" -c \"bgBlueBright.bold,bgMagentaBright.bold\" \"npm:dev-server\" \"npm:dev-client\"",
    "serve": "nodemon -r newrelic index.mjs",
    "build": "parcel build assets/scripts/main.tsx --dist-dir build --public-url /assets",
    "postbuild": "npm run sentry:sourcemaps",
    "dev-client": "parcel watch assets/scripts/main.tsx --dist-dir build --public-url /assets",
    "dev-server": "nodemon -r dotenv/config --watch app --watch lib --watch index.mjs --watch app.mjs index.mjs",
    "test": "npm run lint && npm run jest:watch",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "stylelint \"./assets/**/*.scss\"",
    "lint:js": "eslint --ext .jsx,.js,.tsx,.ts,.mjs,.cjs .",
    "jest": "jest --collectCoverage",
    "jest:watch": "jest --watch",
    "translations:download": "node -r dotenv/config packages/i18n/download_translations.mjs",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:create": "npx sequelize-cli db:create",
    "pretest:models": "NODE_ENV=test npm run db:migrate",
    "test:models": "NODE_ENV=test jest --watchAll --testPathPattern=app/db/models/_tests_",
    "posttest:models": "NODE_ENV=test npm run db:reset",
    "prepare": "husky install",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org streetmix --project streetmix ./build && sentry-cli sourcemaps upload --org streetmix --project streetmix ./build"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "url": "https://streetmix.net/"
    },
    "roots": [
      "app",
      "assets/scripts",
      "test"
    ],
    "setupFilesAfterEnv": [
      "./test/setup-jest.js"
    ],
    "testPathIgnorePatterns": [
      "./test/integration",
      "./app/resources/v1"
    ],
    "collectCoverageFrom": [
      "app.mjs",
      "app/**/*.{js,mjs,cjs}",
      "assets/scripts/**/*.{js,jsx,ts,tsx}",
      "!assets/scripts/vendor/**/*.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|svg)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/test/__mocks__/styleMock.js"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx",
      "node",
      "mjs",
      "cjs"
    ],
    "resolver": "<rootDir>/test/resolver.js",
    "transformIgnorePatterns": [
      "node_modules/(?!react-leaflet)/"
    ]
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead"
  ],
  "postcss": {
    "modules": false,
    "plugins": {
      "postcss-round-subpixels": {}
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,cjs,mjs}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,scss,css,md}": [
      "prettier --write"
    ]
  },
  "alias": {
    "process": {
      "global": "process"
    }
  },
  "@parcel/resolver-default": {
    "packageExports": true
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.5.1",
    "@fortawesome/free-brands-svg-icons": "6.5.1",
    "@fortawesome/free-regular-svg-icons": "6.5.1",
    "@fortawesome/free-solid-svg-icons": "6.5.1",

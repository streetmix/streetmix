{
  "name": "streetmix",
  "version": "1.3.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/streetmix/streetmix.git"
  },
  "license": "BSD-3-Clause",
  "engines": {
    "node": "8.11.1",
    "npm": "6.0.0"
  },
  "scripts": {
    "prestart": "npm run mongo:start",
    "start": "npm run app:start",
    "stop": "npm run mongo:stop",
    "test": "npm run lint && npm run jest:watch",
    "pretest:full": "npm run mongo:start && npm run protractor:update && npm run test:server",
    "test:full": "npm run jest && npm run lint && npm run protractor:local",
    "posttest:full": "npm run test:cleanup",
    "precommit": "lint-staged",
    "mongo:start": "mongod --fork --logpath /dev/null",
    "mongo:stop": "mongo admin --eval 'db.shutdownServer()'",
    "app:start": "supervisor -e node,js,jsx index.js",
    "test:cleanup": "npm run test:server:close && npm run mongo:stop",
    "test:server": "NODE_ENV=test PORT=3000 node index &",
    "test:server:close": "pkill -SIGINT streetmix",
    "lint": "npm run lint:css && npm run lint:js && npm run lint:json",
    "lint:css": "stylelint ./assets/css/**/*.scss",
    "lint:js": "eslint \"**/*.js\" \"**/*.jsx\"",
    "lint:json": "find assets -name '*.json' -exec jsonlint {} --quiet \\;",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "protractor:update": "./node_modules/protractor/bin/webdriver-manager update",
    "protractor:local": "protractor ./test/integration/local.conf.js",
    "translations:download": "node bin/download_translations.js"
  },
  "jest": {
    "roots": [
      "app",
      "assets/scripts"
    ],
    "setupFiles": [
      "./test/setup-jest.js"
    ]
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "not IE <= 10"
  ],
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.jsx": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "jsonlint --in-place",
      "git add"
    ],
    "*.scss": [
      "stylelint --syntax=scss"
    ]
  },
  "dependencies": {
    "@streetmix/icons": "0.4.3",
    "@streetmix/illustrations": "0.3.8",
    "async": "2.6.0",
    "auth0": "2.9.1",
    "auth0-js": "9.5.1",
    "autoprefixer": "8.3.0",
    "axios": "0.18.0",
    "babel-core": "6.26.0",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babelify": "8.0.0",
    "browserify": "16.2.0",
    "browserify-middleware": "8.1.0",
    "btoa": "1.2.1",
    "canvas": "1.6.10",
    "chalk": "2.4.1",
    "compression": "1.7.2",
    "config": "1.30.0",
    "cookie-parser": "1.4.3",
    "cookie-session": "2.0.0-beta.3",
    "cors": "2.8.4",
    "cssnano": "3.10.0",
    "dateformat": "3.0.3",
    "envify": "4.1.0",
    "express": "4.16.3",
    "file-saver": "1.3.8",
    "gulp-if": "2.0.2",
    "gulp-plumber": "1.2.0",
    "gulp-postcss": "7.0.1",
    "gulp-sass": "4.0.1",
    "gulp-sourcemaps": "2.6.4",
    "gulp-tap": "1.0.1",
    "handjs": "1.3.11",
    "hbs": "4.0.1",
    "helmet": "3.12.0",
    "js-cookie": "2.2.0",
    "leaflet": "1.3.1",
    "lodash": "4.17.10",
    "moment": "2.22.1",
    "mongoose": "5.0.16",
    "newrelic": "4.1.0",
    "node-env-file": "0.1.8",
    "node-sass": "4.8.3",
    "oauth": "0.9.15",
    "postcss": "6.0.21",
    "postcss-import": "11.1.0",
    "prop-types": "15.6.1",
    "raven-js": "3.24.2",
    "react": "16.3.2",
    "react-autosuggest": "9.3.4",
    "react-dom": "16.3.2",
    "react-intl": "2.4.0",
    "react-leaflet": "1.9.1",
    "react-redux": "5.0.7",
    "react-transition-group": "2.3.1",
    "redux": "4.0.0",
    "redux-thunk": "2.2.0",
    "request": "2.85.0",
    "requireindex": "1.2.0",

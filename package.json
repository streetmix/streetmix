{
  "name": "streetmix",
  "version": "1.3.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/streetmix/streetmix.git"
  },
  "license": "BSD-3-Clause",
  "engines": {
    "node": "8.1.4",
    "npm": "5.3.0"
  },
  "scripts": {
    "prestart": "npm run mongo:start",
    "start": "supervisor -e node,js,jsx,scss index.js",
    "stop": "npm run mongo:stop",
    "test": "npm run lint && npm run jest:watch",
    "pretest:full": "npm run mongo:start && npm run protractor:update && npm run test:server",
    "test:full": "npm run jest && npm run lint && npm run protractor:local",
    "posttest:full": "npm run test:cleanup",
    "mongo:start": "mongod --fork --logpath /dev/null",
    "mongo:stop": "mongo admin --eval 'db.shutdownServer()'",
    "test:cleanup": "npm run test:server:close && npm run mongo:stop",
    "test:server": "NODE_ENV=test PORT=3000 node index &",
    "test:server:close": "pkill -SIGINT streetmix",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "stylelint ./assets/css/**/*.scss",
    "lint:js": "eslint \"**/*.js\" \"**/*.jsx\"",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "protractor:update": "./node_modules/protractor/bin/webdriver-manager update",
    "protractor:local": "protractor ./test/integration/local.conf.js",
    "translations:download": "node bin/download_translations.js"
  },
  "jest": {
    "roots": [
      "app",
      "assets/scripts"
    ],
    "setupFiles": [
      "./test/shims/raf.js",
      "./test/setup-jest.js"
    ]
  },
  "browserslist": [
    "last 2 versions",
    "IE 11"
  ],
  "dependencies": {
    "async": "2.6.0",
    "autoprefixer": "7.2.3",
    "babel-core": "6.26.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babelify": "8.0.0",
    "body-parser": "1.18.2",
    "browserify": "14.5.0",
    "browserify-middleware": "8.0.0",
    "btoa": "1.1.2",
    "compression": "1.7.1",
    "config": "1.28.1",
    "cookie-parser": "1.4.3",
    "cookie-session": "2.0.0-beta.3",
    "cssnano": "3.10.0",
    "dateformat": "3.0.2",
    "envify": "4.1.0",
    "express": "4.16.2",
    "file-saver": "1.3.3",
    "gulp-if": "2.0.2",
    "gulp-plumber": "1.1.0",
    "gulp-postcss": "7.0.0",
    "gulp-sass": "3.1.0",
    "gulp-sourcemaps": "2.6.1",
    "gulp-tap": "1.0.1",
    "handjs": "1.3.11",
    "helmet": "3.9.0",
    "i18next": "10.2.1",
    "i18next-xhr-backend": "1.5.0",
    "jade": "1.11.0",
    "js-cookie": "2.2.0",
    "kerberos": "0.0.23",
    "leaflet": "1.2.0",
    "lodash": "4.17.4",
    "moment": "2.19.4",
    "mongoose": "4.13.7",
    "newrelic": "2.4.2",
    "node-env-file": "0.1.8",
    "node-sass": "4.7.2",
    "oauth": "0.9.15",
    "postcss": "6.0.14",
    "postcss-import": "11.0.0",
    "prop-types": "15.6.0",
    "raven-js": "3.21.0",
    "react": "16.2.0",
    "react-autosuggest": "9.3.2",
    "react-dom": "16.2.0",
    "react-leaflet": "1.7.8",
    "react-redux": "5.0.6",
    "redux": "3.7.2",
    "request": "2.83.0",
    "requireindex": "1.1.0",
    "sendgrid": "2.0.0",
    "shifty": "2.3.1",
    "sprintf": "0.1.5",
    "streetmix-icons": "git://github.com/streetmix/icons.git#v0.4.2",
    "streetmix-illustrations": "git://github.com/streetmix/illustrations.git",
    "superagent": "3.8.2",
    "supervisor": "0.12.0",
    "twitter": "1.7.1",
    "uuid": "3.1.0",
    "validator": "9.2.0",

    "react-autosuggest": "9.4.3",
    "react-dnd": "9.4.0",
    "react-dnd-cjs": "9.4.0",
    "react-dnd-html5-backend": "9.4.0",
    "react-dnd-html5-backend-cjs": "9.4.0",
    "react-dnd-multi-backend": "4.0.0",
    "react-dnd-touch-backend": "9.4.0",

{
  "name": "streetmix",
  "version": "1.3.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/streetmix/streetmix.git"
  },
  "license": "AGPL-3.0-or-later",
  "engines": {
    "node": "16.x",
    "npm": ">=8.x"
  },
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "postinstall": "node app/bundle.js",
    "start": "nodemon -r dotenv/config --watch app --watch lib --watch index.js --watch app.js index.js",
    "test": "npm run lint && npm run jest:watch",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "stylelint \"./assets/**/*.scss\"",
    "lint:js": "eslint --ext .jsx,.js .",
    "jest": "jest --collectCoverage",
    "jest:watch": "jest --watch",
    "translations:download": "node -r dotenv/config bin/download_translations.js",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:create": "npx sequelize-cli db:create",
    "pretest:models": "NODE_ENV=test npm run db:migrate",
    "test:models": "NODE_ENV=test jest --watchAll --testPathPattern=app/db/models/_tests_",
    "posttest:models": "NODE_ENV=test npm run db:reset",
    "prepare": "husky install"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "roots": [
      "app",
      "assets/scripts",
      "test"
    ],
    "setupFilesAfterEnv": [
      "./test/setup-jest.js"
    ],
    "testPathIgnorePatterns": [
      "./test/integration"
    ],
    "collectCoverageFrom": [
      "app.js",
      "app/**/*.{js,jsx}",
      "assets/scripts/**/*.{js,jsx}",
      "!assets/scripts/vendor/**/*.{js,jsx}"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|svg)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/test/__mocks__/styleMock.js"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx",
      "node",
      "mjs"
    ]
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead"
  ],
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {},
      "postcss-round-subpixels": {}
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,scss,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@formatjs/intl-relativetimeformat": "9.5.0",

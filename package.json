{
  "name": "streetmix",
  "version": "1.3.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/streetmix/streetmix.git"
  },
  "license": "BSD-3-Clause",
  "engines": {
    "node": "10.11.0",
    "npm": "6.4.1"
  },
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "postinstall": "node app/bundle.js",
    "start": "supervisor --extensions js --ignore .cache,assets,build,test index.js",
    "test": "npm run lint && npm run jest:watch",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "stylelint \"./assets/**/*.scss\"",
    "lint:js": "eslint \"**/*.js\" \"**/*.jsx\"",
    "jest": "jest --collectCoverage",
    "jest:watch": "jest --watch",
    "translations:download": "node bin/download_translations.js",
    "docs": "npm run docs:build && npm run docs:serve",
    "docs:build": "cd docs && make dirhtml",
    "docs:serve": "http-server docs/_build/dirhtml"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "roots": [
      "app",
      "assets/scripts",
      "test"
    ],
    "setupFilesAfterEnv": [
      "./test/setup-jest.js"
    ],
    "testPathIgnorePatterns": [
      "./test/integration"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "app.js",
      "app/**/*.{js,jsx}",
      "assets/scripts/**/*.{js,jsx}",
      "!assets/scripts/vendor/**/*.{js,jsx}"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|svg)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/test/__mocks__/styleMock.js",
      "^dnd-core$": "dnd-core/dist/cjs",
      "^react-dnd$": "react-dnd/dist/cjs",
      "^react-dnd-html5-backend$": "react-dnd-html5-backend/dist/cjs",
      "^react-dnd-touch-backend$": "react-dnd-touch-backend/dist/cjs",
      "^react-dnd-test-backend$": "react-dnd-test-backend/dist/cjs",
      "^react-dnd-test-utils$": "react-dnd-test-utils/dist/cjs"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx",
      "node",
      "mjs"
    ]
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "not IE <= 10"
  ],
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {}
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{json,scss,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@formatjs/intl-relativetimeformat": "4.2.0",
    "@fortawesome/fontawesome-svg-core": "1.2.25",
    "@fortawesome/free-brands-svg-icons": "5.11.2",
    "@fortawesome/free-regular-svg-icons": "5.11.2",
    "@fortawesome/free-solid-svg-icons": "5.11.2",
    "@fortawesome/react-fontawesome": "0.1.7",
    "@sentry/browser": "5.7.1",
    "@streetmix/icons": "0.7.0",
    "@streetmix/illustrations": "0.9.0",
    "async": "3.1.0",
    "auth0": "2.20.0",
    "auth0-js": "9.11.3",
    "autoprefixer": "9.7.1",
    "axios": "0.19.0",
    "btoa": "1.2.1",
    "chalk": "2.4.2",
    "cloudinary": "1.16.0",
    "compression": "1.7.4",
    "config": "3.2.4",
    "cookie-parser": "1.4.4",
    "cookie-session": "2.0.0-beta.3",
    "copy-to-clipboard": "3.2.0",
    "core-js": "3.3.4",
    "cors": "2.8.5",
    "dateformat": "3.0.3",
    "dotenv": "8.2.0",
    "downshift": "3.4.1",
    "downshift-pelias": "0.0.3-alpha",
    "express": "4.17.1",
    "file-saver": "2.0.2",
    "handjs": "1.3.11",
    "hbs": "4.0.6",
    "helmet": "3.21.2",
    "intl-messageformat": "7.5.0",
    "intl-pluralrules": "1.1.1",
    "js-cookie": "2.2.1",
    "json2csv": "4.5.4",
    "leaflet": "1.5.1",
    "lodash": "4.17.15",
    "mongoose": "5.7.5",
    "newrelic": "6.0.0",
